SELECT ename 
FROM employee 
WHERE job = (SELECT jcode 
             FROM job
             WHERE name = 'Staff')
             and salary > any (SELECT salary 
                              FROM employee
                              WHERE job = (SELECT jcode 
                               FROM job
                               WHERE name = 'Clerk'))

out put:-

ENAME
Krishna
Deepa
Keerthi
Srikanth


4) display the names of clerks who earn a salary more than lowest salary of any employee whose job role is staff

SELECT ename 
FROM employee 
WHERE job = (SELECT jcode 
             FROM job
             WHERE name = 'clerk')
             and salary > (SELECT min(salary) 
                              FROM employee
                              WHERE job = (SELECT jcode 
                               FROM job
                               WHERE name = 'staff'))

out put:-
no data found

5) select e name,sal,deptno from emp where sail in (select max(sal) from emp group deptno

 
SELECT ename,salary,deptno
FROM employee e
where salary =(select max(salary)
           FROM employee 
           where deptno = e.deptno)

out put:-
ENAME	SALARY	DEPTNO
Venkat	1200000	40
Nirmala	800000	20
Srinivas	1000000	30
Krishna	500000	12
Deepa	600000	23
Keerthi	600000	24
Srikanth	400000	34



6) display the names of the employees who earn highest salaries in their respective job groups


SELECT ename,salary,job
FROM employee e
where salary =(select max(salary)
           FROM employee 
           where job = e.job)


ENAME	SALARY	JOB
Venkat	1200000	672
